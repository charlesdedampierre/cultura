    "            \n",
    "            # European countries\n",
    "            european_countries = ['United Kingdom', 'Germany', 'France', 'Italy', 'Spain', \n",
    "                                 'Netherlands', 'Switzerland', 'Belgium', 'Austria', 'Greece',\n",
    "                                 'Sweden', 'Norway', 'Denmark', 'Finland', 'Portugal', 'Ireland',\n",
    "                                 'Poland', 'Russia', 'Czech Republic', 'Hungary', 'Romania']\n",
