    "        # Print immediate result\n",
    "        print(f\"  Result: {classification if classification else 'None'}\")\n",
    "        print(f\"  Explanation: {explanation}\")\n",
    "        print(f\"  Sources: {sources}\")\n",
    "        \n",
    "        # Add a delay to avoid rate limiting\n",
    "        # time.sleep(1)\n",
    "    \n",
